@model IEnumerable<ECommerceVersionFinal.LigneCommande>

@{
    ViewBag.Title = "Index";
}

<h2>Les produits </h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.quantite)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.prix)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Commande.User_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Panier.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Produit.nom)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.quantite)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.prix)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Commande.User_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Panier.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Produit.nom)
            </td>
            
        </tr>
    }

</table>
