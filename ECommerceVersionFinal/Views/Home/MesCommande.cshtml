@model IEnumerable<ECommerceVersionFinal.Commande>

@{
    ViewBag.Title = "MesCommande";
}

<h2>Mes Commandes</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.dateCommande)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.dateCommande)
        </td>
        
        <td>
           
            @Html.ActionLink("Details", "DetailMesCommande", new { id=item.Id }) 
          
        </td>
    </tr>
}

</table>
